@{
    ViewData["Title"] = "Clientes";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">CPF</th>
            <th scope="col">Nome</th>
            <th scope="col">Endereço</th>
            <th scope="col">Telefone</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (Cliente cliente in Empresa.Clientes)
            {
                <tr>
                    <td>@cliente.CPF</td>
                    <td>@cliente.Nome</td>
                    <td>@cliente.Endereco</td>
                    <td>@cliente.Telefone</td>
                    <td>@cliente.Email</td>
                </tr>
            }
        }
    </tbody>
</table>
